<div id="wrapper">

    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Dashboard</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
<div class="row">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <i class="fa fa-bar-chart-o fa-fw"></i> Statistics
                        <div class="pull-right">
                            <div class="btn-group">
                                <button type="button" class="btn btn-default btn-xs dropdown-toggle menu"
                                    data-toggle="dropdown">
                                    Date
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu pull-right barchart" role="menu">
                                    <li><a href="javascript:m.setData(jsonBar('Date'));">Date</a>
                                    </li>
                                    <li><a href="javascript: m.setData(jsonBar('Week'));">Week</a>
                                    </li>
                                    <li><a href="javascript: m.setData(jsonBar('Month'));">Month</a>
                                    </li>
                                    <li><a href="javascript: m.setData(jsonBar('Quarter'));">Quarter</a>
                                    </li>
                                    <li><a href="javascript: m.setData(jsonBar('Year'));">Year</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="table-responsive">
                                    
                                </div>
                                <!-- /.table-responsive -->
                            </div>
                            <!-- /.col-lg-4 (nested) -->
                            <div class="col-lg-8 ">
                                <div id="morris-bar-chart"></div>
                            </div>
                            <!-- /.col-lg-8 (nested) -->
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.panel-body -->
                </div>
            </div>
            <!-- /.row -->
            <div class="row">
                
                <!-- /.col-lg-8 -->
                <div class="col-lg-4 col-lg-offset-4">

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i href="javascript:load();" class="fa fa-bar-chart-o fa-fw"></i> Top 10 Product Selling

                        </div>
                        <div class="panel-body">
                            <div id="morris-donut-chart"></div>
                        </div>
                        <!-- /.panel-body -->
                    </div>

                    <!-- /.panel .chat-panel -->
                </div>
                <!-- /.col-lg-4 -->
            </div>
            
            <!-- /.row -->
        </div>
        <!-- /.container-fluid -->
    </div>
    <!-- /#page-wrapper -->

</div>
<!-- /#wrapper -->

<!-- jQuery -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src='../js/handlebars-v4.7.6.js' type="text/javascript"></script>
<script id="list-template" type="text/x-handlebars">
    <table class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <th>#</th>
                    <th>\{{time}}</th>
                    <th>Total</th>
                </tr>
            </thead>
            \{{#each revenue}}
            <tbody>
                <tr>
                    <td>\{{id}}</td>
                    <td>\{{date}}</td>
                    <td>\{{money}}</td>
                </tr>
            </tbody>
            \{{/each}}
        </table>
</script>


<!-- Bootstrap Core JavaScript -->
<script src="../js/bootstrap.min.js"></script>

<!-- Metis Menu Plugin JavaScript -->
<script src="../js/metisMenu.min.js"></script>

<!-- Morris Charts JavaScript -->
<script src="../js/raphael.min.js"></script>
<script src="../js/morris.min.js"></script>
<script src="../js/morris-data.js"></script>


<!-- Custom Theme JavaScript -->
<script src="../js/startmin.js"></script>
<script>
    $(".dropdown-menu li a").click(function () {
        $(".btn:first-child.menu").text($(this).text());
        $(".btn:first-child.menu").val($(this).text());
    });
</script>
